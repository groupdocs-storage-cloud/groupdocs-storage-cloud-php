{
    "description": 	"This repository contains GroupDocs Storage Cloud SDK for PHP source code. GroupDocs Storage Cloud SDK for PHP is a true REST API that provides managers for multiple storage types.",
    "name": 		"groupdocs-storage-cloud/php-sdk",
    "type": 		"library",
    "keywords" : 	["groupdocs", "php", "groupdocs storage cloud"],
    "homepage": "https://github.com/groupdocs-storage-cloud/groupdocs-storage-cloud-php",
    "authors": [
        {
            "name": "Ciocan Madalina",
			"homepage": "https://github.com/groupdocs-storage-cloud"
        }
    ],
	"config": {
        "platform": {
            "php": "5.6.32"
        }
    },
    "require": {
        "php": ">=5.5",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2",
        "friendsofphp/php-cs-fixer": "~1.12"
    },
    "require-dev": {
        "squizlabs/php_codesniffer": "3.*",
        "phpunit/phpunit": "^4.8"
    },
	"autoload": {
        "psr-0": {
            "GroupDocs": "src/",
            "BaseTest": "tests/"
        }
    }
}
